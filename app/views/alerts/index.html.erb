		  <th>Alert</th>
		  <th>Status</th>
		  <th>Address</th>
		  <th>Notes</th>
		  <th>Phone #</th>
		  <th colspan="3"></th>
		</tr>
	</thead>
	<% @alerts.each do |alert| %>
		<tr>
			<td><%= alert.id %></td>
